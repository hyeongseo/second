<h1>글쓰기</h1>
<div>
  <%= form_for :content, url: contents_path do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :body %><br>
      <%= f.text_area :body %>
    </p>
    <p>
      <%= f.submit %>
    </p>
  <% end %>
</div>

<% link_to 'Back', contents_path %>
